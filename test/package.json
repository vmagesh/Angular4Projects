{
  "name": "catch-up-enterprise",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^8.1.0",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/fire": "^5.4.2",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.1.0",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@angular/service-worker": "^8.1.0",
    "@fortawesome/fontawesome-free": "^5.1.0",
    "@fullcalendar/angular": "^4.4.5-beta",
    "@fullcalendar/core": "^4.4.0",
    "@fullcalendar/daygrid": "^4.4.0",
    "@fullcalendar/interaction": "^4.4.0",
    "@fullcalendar/timegrid": "^4.4.0",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "@ngx-translate/core": "^11.0.1",
    "@types/aes-js": "^3.1.0",
    "@types/socket.io-client": "^1.4.32",
    "aes-js": "^3.1.2",
    "bootstrap": "^4.1.1",
    "bootstrap-datepicker": "^1.9.0",
    "firebase": "^7.9.3",
    "fsevents": "^2.1.2",
    "google-libphonenumber": "^3.2.7",
    "intl-tel-input": "^14.1.0",
    "jquery": "^3.4.1",
    "@types/jquery": "3.3.33",
    "ng-bootstrap-datetime-angular": "0.1.25",
    "ng-otp-input": "^1.7.0",
    "ngx-bootstrap": "^5.5.0",
    "ng4-loading-spinner": "^1.2.3",
    "ngx-chips": "2.1.0",
    "ngx-countdown": "^8.0.3",
    "ngx-international-phone-number": "^1.0.6",
    "ngx-intl-tel-input": "^2.3.1",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.4",
    "socket.io-client": "^2.3.0",
    "sweetalert2": "^9.8.2",
    "angular-ng-autocomplete": "2.0.1",
    "zone.js": "^0.9.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.8",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@angularclass/hmr": "^2.1.3",
    "fsevents": "*",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.17.17",
    "codelyzer": "^5.1.0",
    "hads": "^3.0.3",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^2.0.0",
    "husky": "^3.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "^3.0.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.4",
    "puppeteer": "^1.17.0",
    "stylelint": "~10.1.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended-scss": "~3.3.0",
    "stylelint-config-standard": "~18.3.0",
    "stylelint-scss": "~3.9.0",
    "ts-node": "^8.3.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.4.0"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
